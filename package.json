{
  "name": "weatherapp-boilerplate",
  "version": "1.1.1",
  "description": "Preact starter project for a weather app powered by webpack.",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress",
    "start": "serve build -s -c 1",
    "prestart": "npm run build",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "prebuild": "mkdirp build && ncp src/assets build/assets",
    "test": "npm run -s lint && npm run -s test:karma",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "lint": "eslint {src,test}"
  },
  "keywords": [
    "preact",
    "boilerplate",
    "webpack"
  ],
  "license": "MIT",
  "author": "Jason Miller <jason@developit.ca>",
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel": "^6.5.2",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.4",
    "eslint": "^3.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "isparta-loader": "^2.0.0",
    "jquery": "3.1.1",
    "json-loader": "^0.5.7",
    "karma": "^1.0.0",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^2.0.13",
    "less": "^2.7.3",
    "less-loader": "^2.2.3",
    "mkdirp": "^0.5.6",
    "mocha": "^3.0.2",
    "ncp": "^2.0.0",
    "offline-plugin": "^4.5.5",
    "phantomjs-prebuilt": "^2.1.12",
    "postcss-loader": "^1.2.2",
    "raw-loader": "^0.5.1",
    "replace-bundle-webpack-plugin": "^1.0.0",
    "script-ext-html-webpack-plugin": "^1.3.5",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "source-map-loader": "^0.1.6",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.9",
    "v8-lazy-parse-webpack-plugin": "^0.3.0",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.15.2"
  },
  "dependencies": {
    "preact": "^6.0.2",
    "preact-compat": "^3.0.1",
    "preact-router": "^2.0.0",
    "promise-polyfill": "^6.0.2",
    "proptypes": "^0.14.4",
    "serve": "^2.0.0"
  },
  "main": "webpack.config.babel.js",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Juan-Alvarado/weatherapp-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/Juan-Alvarado/weatherapp-boilerplate/issues"
  },
  "homepage": "https://github.com/Juan-Alvarado/weatherapp-boilerplate#readme"
}
